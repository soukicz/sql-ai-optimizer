{
    "name": "soukicz/sql-ai-optimizer",
    "type": "project",
    "autoload": {
        "psr-4": {
            "Soukicz\\SqlAiOptimizer\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Petr Soukup",
            "email": "soukup@simplia.cz"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-mysqli": "*",
        "ext-sqlite3": "*",
        "soukicz/llm": "dev-master",
        "symfony/framework-bundle": "^7.2",
        "symfony/runtime": "^7.2",
        "dibi/dibi": "^5.0",
        "symfony/dotenv": "^7.2",
        "symfony/cache": "^7.2",
        "twig/twig": "^3.20",
        "league/commonmark": "^2.6",
        "spatie/commonmark-highlighter": "^3.0"
    },
    "config": {
        "allow-plugins": {
            "symfony/runtime": true
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1"
    }
}
